/* pages/index/index.wxss */
@import '../template/tabbar.wxss';

page {
  padding-top: 130rpx;
}

.layout .message {
  width: 100%;
  height: 140rpx;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
}

.layout .message .message_List .head {
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  position: relative;
  border: 1px solid transparent;
  background-clip: padding-box;
}

.layout .message .message_List .head::after {
  content: '';
  position: absolute;
  top: -4rpx;
  bottom: -4rpx;
  left: -4rpx;
  right: -4rpx;
  background: linear-gradient(to bottom, #2d5ad1, #63c4de);
  z-index: -1;
  border-radius: 50%;
}

.layout .message .message_List .head image {
  width: 100%;
  height: 100%;
  display: block;
  border-radius: 50%;
}

.layout .message .message_List {
  height: 110rpx;
  padding: 15rpx 30rpx;
  display: flex;
  align-items: center;
}

.layout .message .message_List .name {
  color: #fff;
}

.layout .message .message_List .data {
  color: #64cbf3;
  font-size: 24rpx;
  font-weight: 600;
}

.layout .message .message_List .code {
  height: 50rpx;
  flex: 1;
  text-align: right;
  color: #64cbf3;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  font-size: 24rpx;
  font-weight: 600;
}

.layout .message .message_List .code image {
  width: 43rpx;
  height: 43rpx;
  display: block;
  margin-bottom: 4rpx;
}

.layout .message .messageDetails {
  height: 78rpx;
  padding: 6rpx 0;
  margin-left: 24rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.layout .message .messageDetails .data {
  display: flex;
  align-items: center;
}

.layout .message .messageDetails .data image {
  width: 30rpx;
  height: 30rpx;
  display: block;
  margin-right: 6rpx;
}

.layout .selectList .box {
  width: 614rpx;
  height: 162rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
  background-position: top;
  background-repeat: no-repeat;
  margin: 20rpx auto 0;
  filter: brightness(150%);
  display: flex;
  padding: 0 30rpx 0 40rpx;
}

.selectList .box .gameName {
  height: 100rpx;
  font-size: 34rpx;
  color: #fff;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-top: 28rpx;
}

.selectList .box .gameName .data {
  font-size: 22rpx;
  color: #64cbf3;
  display: flex;
  align-items: center;
}

.selectList .box .gameName .data .txt {
  width: 340rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
}

.selectList .box .gameName .data .rule {
  color: #fff;
  width: 120rpx;
  height: 40rpx;
  line-height: 40rpx;
  border: 1px solid #135f90;
  text-align: center;
  font-size: 24rpx;
  font-weight: 600;
  margin-left:10rpx;
}

.selectList .box .start {
  flex: 1;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  margin-top: 82rpx;
  color: #fff;
  font-size: 24rpx;
  font-weight: 600;
  margin-left: 8rpx;
}

.loading {
  width: 100%;
  height: 100%;
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9999;
}

.loading .Box {
  width: 220rpx;
  height: 220rpx;
  /* background: rgba(74, 74, 74, 0.1); */
  border-radius: 10rpx;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -220rpx;
  margin-left: -95rpx;
}

.load {
  width: 100rpx;
  height: 100rpx;
  position: relative;
  margin: 0 auto;
}

.load image{
  width:100%;
  height:100%;
  display:block;
}


