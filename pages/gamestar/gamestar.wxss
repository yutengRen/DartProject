/* pages/gamestar/gamestar.wxss */

/* 游戏中动画效果 */

.layout .content {
  width: 400rpx;
  height: 66rpx;
  line-height: 66rpx;
  color: #fff;
  text-align: center;
  font-size: 30rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
  background-position: top;
  background-repeat: no-repeat;
  margin: 100rpx auto 0;
  filter: brightness(150%);
}

.border {
  width: 350rpx;
  position: relative;
  margin: 0 auto;
}

.box {
  width: 178rpx;
  height: 178rpx;
  animation: play 1s linear infinite;
  margin: 100rpx auto 0;
}

.box:before {
  content: "";
  display: block;
  width: 180rpx;
  height: 90rpx;
  padding: 30rpx;
  padding-bottom: 0;
  box-sizing: border-box;
  border-top-left-radius: 90rpx;
  border-top-right-radius: 90rpx;
  background: -webkit-gradient(linear, left top, right top, color-stop(0, #fff), color-stop(1, #fff)),
                -webkit-gradient(linear, left top, right top, color-stop(0, #6be2f4), color-stop(1, #151f4c));
  background-clip: content-box, padding-box;
}

.box:after {
  content: "";
  display: block;
  width: 180rpx;
  height: 90rpx;
  padding: 30rpx;
  padding-top: 0;
  box-sizing: border-box;
  border-bottom-left-radius: 90rpx;
  border-bottom-right-radius: 90rpx;
  background: -webkit-gradient(linear, left top, right top, color-stop(0, #fff), color-stop(1, #fff)),
                -webkit-gradient(linear, left top, right top, color-stop(0, #e237a4), color-stop(1, #151f4c));
  background-clip: content-box, padding-box;
}

@keyframes play {
  from {
    transform: rotate(0deg);
  }

  to {
    transform: rotate(360deg);
  }
}

.border image {
  width: 142rpx;
  height: 142rpx;
  border-radius: 50%;
  display: block;
  position: absolute;
  top: 19rpx;
  left: 105rpx;
}

.over {
  width: 400rpx;
  height: 80rpx;
  border: 1px solid #63c4de;
  text-align: center;
  line-height: 80rpx;
  margin: 500rpx auto 0;
  box-shadow: 0px 0px 6px 0px #d6c796;
  color: #fff;
  background: rgba(20, 40, 86, 0.1);
  font-weight: 700;
  font-size: 34rpx;
}
